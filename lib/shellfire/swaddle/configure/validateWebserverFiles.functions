configure_validateWebserverFilesDigest()
{
	case "$1" in

		md5|sha1|sha256|sha384|sha512|gpg)
			:
		;;

		*)
			core_exitError $core_commandLine_exitCode_CONFIG "The configuration setting '$configurationSettingName' ('$1') is not a valid digest (it must be one of 'md5', 'sha1', 'sha256', 'sha384', 'sha512' or 'sign')."
		;;
		
	esac
}
