debian_control_parser_paragraph_copyright()
{
	_debian_control_parser_paragraph_copyright_headerStartOfParagraphEvent
}

_debian_control_parser_paragraph_copyright_headerStartOfParagraphEvent()
{
	debian_control_parser_raiseStartOfFieldEvent=_debian_control_parser_paragraph_copyright_headerStartOfFieldEvent
	debian_control_parser_raiseContinuationOfFieldEvent=debian_control_parser_continuationEventNotWanted
	debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
	debian_control_parser_raiseEndOfParagraphEvent=_debian_control_parser_paragraph_copyright_headerEndOfParagraphEvent

	core_variable_array_append debian_control_parser_mandatoryFields 'Format'
	
	# FileFormat ParagraphType EventType EventSubType
	${debian_control_parser_eventHandler} Copyright Header ParagraphStart
}

# Now we need to deal with mandatory fields
_debian_control_parser_paragraph_copyright_headerStartOfFieldEvent()
{
	case "$debian_control_parser_fieldName" in
		
		Format)
			if [ "$debian_control_parser_fieldValueFirstLine" != 'http://www.debian.org/doc/packaging-manuals/copyright-format/1.0/' ]; then
				debian_control_parser_warning "Unknown copyright field 'Format' '$debian_control_parser_fieldValueFirstLine'"
			fi
			
			debian_control_parser_fieldType=singleLine
			debian_control_parser_raiseContinuationOfFieldEvent=debian_control_parser_continuationEventNotWanted
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
		;;
	
		Upstream-Name)
			debian_control_parser_fieldType=singleLine
			debian_control_parser_raiseContinuationOfFieldEvent=debian_control_parser_continuationEventNotWanted
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
		;;
	
		Upstream-Contact)
			debian_control_parser_fieldType=multilineList
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
		;;
	
		Source)
			debian_control_parser_fieldType=multilineWithoutSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
		;;
	
		Disclaimer)
			debian_control_parser_fieldType=multilineWithoutSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
		;;
	
		Comment)
			debian_control_parser_fieldType=multilineWithoutSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
		;;
	
		License)
			debian_control_parser_fieldType=multilineWithSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
		;;
		
		Copyright)
			debian_control_parser_fieldType=multilineWithoutSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent
		;;
		
		*)
			debian_control_parser_warning "Unknown field '$debian_control_parser_fieldName' in Copyright Header paragraph"
		;;
		
	esac
}

_debian_control_parser_paragraph_copyright_headerEndOfFieldEvent()
{
	# FileFormat ParagraphType EventType FieldName FieldType
	${debian_control_parser_eventHandler} Copyright Header Field "$debian_control_parser_fieldName" "$debian_control_parser_fieldType"
}

_debian_control_parser_paragraph_copyright_headerEndOfParagraphEvent()
{
	# FileFormat ParagraphType EventType EventSubType
	${debian_control_parser_eventHandler} Copyright Header ParagraphEnd
	
	debian_control_parser_raiseStartOfParagraphEvent=_debian_control_parser_paragraph_copyright_filesOrLicenseStartOfParagraphEventDiscriminate
}



_debian_control_parser_paragraph_copyright_filesOrLicenseStartOfParagraphEventDiscriminate()
{
	debian_control_parser_raiseStartOfFieldEvent=_debian_control_parser_paragraph_copyright_filesOrLicenseStartOfFieldEventDiscriminate
}

_debian_control_parser_paragraph_copyright_filesOrLicenseStartOfFieldEventDiscriminate()
{
	case "$debian_control_parser_fieldName" in
		
		Files)
			debian_control_parser_raiseStartOfFieldEvent=_debian_control_parser_paragraph_copyright_filesStartOfFieldEvent
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_filesEndOfFieldEvent
			debian_control_parser_raiseEndOfParagraphEvent=_debian_control_parser_paragraph_copyright_filesEndOfParagraphEvent
			core_variable_array_append debian_control_parser_mandatoryFields 'Files' 'Copyright' 'License'

			# FileFormat ParagraphType EventType EventSubType
			${debian_control_parser_eventHandler} Copyright Files ParagraphStart
		;;
		
		License)
			debian_control_parser_raiseStartOfFieldEvent=_debian_control_parser_paragraph_copyright_licenseStartOfFieldEvent
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_licenseEndOfFieldEvent
			debian_control_parser_raiseEndOfParagraphEvent=_debian_control_parser_paragraph_copyright_licenseEndOfParagraphEvent
			core_variable_array_append debian_control_parser_mandatoryFields 'License'
			
			# FileFormat ParagraphType EventType EventSubType
			${debian_control_parser_eventHandler} Copyright License ParagraphStart
		;;
		
		*)
			debian_control_parser_warning "Did not expect a Copyright paragraph starting with the field '$debian_control_parser_fieldName'"
		;;
		
	esac
	
	${debian_control_parser_raiseStartOfFieldEvent} "$@"
}



_debian_control_parser_paragraph_copyright_filesStartOfFieldEvent()
{
	case "$debian_control_parser_fieldName" in
	
		Files)
			debian_control_parser_fieldType=singleLine
			debian_control_parser_raiseContinuationOfFieldEvent=debian_control_parser_continuationEventNotWanted
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_filesEndOfFieldEvent
		;;
		
		Copyright)
			debian_control_parser_fieldType=multilineWithoutSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_filesEndOfFieldEvent
		;;
	
		License)
			debian_control_parser_fieldType=multilineWithSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_filesEndOfFieldEvent
		;;
	
		Comment)
			debian_control_parser_fieldType=multilineWithoutSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_filesEndOfFieldEvent
		;;
		
		*)
			debian_control_parser_warning "Unknown field '$debian_control_parser_fieldName' in Copyright License paragraph"
		;;
		
	esac
}

_debian_control_parser_paragraph_copyright_filesEndOfFieldEvent()
{
	# FileFormat ParagraphType EventType FieldName FieldType
	${debian_control_parser_eventHandler} Copyright Files Field "$debian_control_parser_fieldName" "$debian_control_parser_fieldType"
}

_debian_control_parser_paragraph_copyright_filesEndOfParagraphEvent()
{
	# FileFormat ParagraphType EventType EventSubType
	${debian_control_parser_eventHandler} Copyright Files ParagraphEnd
	
	debian_control_parser_raiseStartOfParagraphEvent=_debian_control_parser_paragraph_copyright_filesOrLicenseStartOfParagraphEventDiscriminate
}



_debian_control_parser_paragraph_copyright_licenseStartOfFieldEvent()
{
	case "$debian_control_parser_fieldName" in
	
		License)
			debian_control_parser_fieldType=multilineWithSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_licenseEndOfFieldEvent
		;;
	
		Comment)
			debian_control_parser_fieldType=multilineWithoutSynopsis
			debian_control_parser_raiseContinuationOfFieldEvent=true
			debian_control_parser_raiseEndOfFieldEvent=_debian_control_parser_paragraph_copyright_licenseEndOfFieldEvent
		;;
		
		*)
			debian_control_parser_warning "Unknown field '$debian_control_parser_fieldName' in Copyright License paragraph"
		;;
		
	esac
}

_debian_control_parser_paragraph_copyright_licenseEndOfFieldEvent()
{
	# FileFormat ParagraphType EventType FieldName FieldType
	${debian_control_parser_eventHandler} Copyright License Field "$debian_control_parser_fieldName" "$debian_control_parser_fieldType"
}

_debian_control_parser_paragraph_copyright_licenseEndOfParagraphEvent()
{
	# FileFormat ParagraphType EventType EventSubType
	${debian_control_parser_eventHandler} Copyright License ParagraphEnd
	
	debian_control_parser_raiseStartOfParagraphEvent=_debian_control_parser_paragraph_copyright_filesOrLicenseStartOfParagraphEventDiscriminate
}
