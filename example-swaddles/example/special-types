# The following special types are supported
swaddle_path_uid=1
swaddle_path_gid=1
swaddle_path_mode=0644
swaddle_path_specialType=package-owned-directory
swaddle_path_specialType=config-file
swaddle_path_specialType=hardlink,/path/to/hardlink
swaddle_path_specialType=character-device,3,4
swaddle_path_specialType=block-device,3,4
swaddle_path_specialType=fifo
